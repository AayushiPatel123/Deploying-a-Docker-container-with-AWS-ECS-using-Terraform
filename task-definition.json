{
    "family": "sonarqube-task",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "sonarqube",
        "image": "123456789012.dkr.ecr.us-west-2.amazonaws.com/custom-sonarqube:latest",
        "memory": 512,
        "portMappings": [
          {
            "containerPort": 9000,
            "hostPort": 9000
          }
        ],
        "essential": true,
        "environment": [
          {
            "name": "SONARQUBE_HOME",
            "value": "/opt/sonarqube"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/sonarqube",
            "awslogs-region": "us-west-2",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::123456789012:role/ecsTaskExecutionRole"
  }
  